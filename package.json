{
  "name": "@flex-development/grease",
  "description": "Package release workflow tool",
  "version": "1.0.0",
  "keywords": [
    "bitbucket",
    "github cli",
    "github release",
    "release notes",
    "standard-version",
    "typescript"
  ],
  "homepage": "https://github.com/flex-development/grease",
  "repository": "git://github.com/flex-development/grease.git",
  "bugs": {
    "url": "https://github.com/flex-development/grease/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevlopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "publishConfig": {
    "access": "public",
    "branch": "main",
    "branch_latest": "next",
    "directory": "./build",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "clean": "rimraf node_modules",
    "postinstall": "husky-run install",
    "check:format": "prettier --check .",
    "check:style": "eslint . --ext md,js,ts.tsx",
    "fix:format": "prettier --write .",
    "fix:style": "yarn check:style --fix --cache",
    "test": "bash scripts/jest.sh",
    "build": "dotenv -c production ts-node ./scripts/build",
    "prepare": "install-peers; yarn build",
    "prepack": "yarn prepare"
  },
  "dependencies": {
    "@flex-development/exceptions": "2.0.0",
    "@flex-development/tutils": "3.1.0",
    "@types/debug": "4.1.5",
    "@types/lodash": "4.14.170",
    "@types/node": "16.4.8",
    "@types/shelljs": "0.8.9",
    "@types/standard-version": "7.0.1",
    "chalk": "4.1.1",
    "debug": "4.3.2",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "shelljs": "0.8.4",
    "standard-version": "9.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@commitlint/cli": "13.0.0",
    "@commitlint/config-conventional": "13.0.0",
    "@commitlint/format": "13.0.0",
    "@types/faker": "5.5.6",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "26.0.23",
    "@types/mvdan-sh": "0.5.0",
    "@types/node": "15.12.5",
    "@types/rimraf": "2.0.4",
    "@types/shelljs": "0.8.8",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "chalk-cli": "4.1.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsdoc": "35.4.0",
    "eslint-plugin-markdown": "2.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "eslint-plugin-tree-shaking": "1.9.2",
    "faker": "5.5.3",
    "fs-extra": "10.0.0",
    "husky": "4.3.8",
    "install-peers-cli": "2.2.0",
    "jest": "27.0.0-next.11",
    "jest-extended": "0.11.5",
    "lint-staged": "11.0.0",
    "mkdirp": "0.5.5",
    "npm": "7.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "prettier-plugin-sh": "0.7.0",
    "replace-in-file": "6.2.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "1.5.12",
    "typescript": "4.3.4"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0",
    "yarn": "3.0.0-rc.2"
  },
  "env": {
    "optional": [
      "DEBUG",
      "DEBUG_COLORS"
    ],
    "required": [
      "GITHUB_PAT_GPR"
    ]
  }
}
