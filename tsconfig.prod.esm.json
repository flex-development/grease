{
  "compilerOptions": {
    "module": "esnext",
    "outDir": "./esm",
    "paths": {
      "@flex-development/dtag": [
        "node_modules/@flex-development/dtag/esm/index"
      ],
      "@flex-development/dtag/*": ["node_modules/@flex-development/dtag/esm/*"],
      "@flex-development/log": [
        "packages/grease/node_modules/@flex-development/log/esm/index"
      ],
      "@flex-development/log/*": [
        "packages/grease/node_modules/@flex-development/log/esm/*"
      ],
      "@grease": ["packages/grease/src/index.ts"],
      "@grease/*": ["packages/grease/src/*"]
    },
    "target": "esnext"
  },
  "extends": "./tsconfig.prod.json"
}
