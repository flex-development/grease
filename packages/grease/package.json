{
  "name": "@flex-development/grease",
  "description": "Package release workflow tool",
  "version": "1.1.0",
  "keywords": [
    "bitbucket",
    "changelog",
    "github cli",
    "github release",
    "release notes",
    "release notes generator",
    "standard-version",
    "typescript"
  ],
  "homepage": "https://github.com/flex-development/grease/tree/next/packages/grease",
  "repository": {
    "type": "git",
    "url": "git://github.com/flex-development/grease.git",
    "directory": "packages/grease"
  },
  "bugs": {
    "url": "https://github.com/flex-development/grease/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevlopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "files": [
    "cjs",
    "esm",
    ".env",
    ".env.production",
    "CHANGELOG.md"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "test": "bash ../../scripts/jest.sh",
    "build": "dotenv -c production -- ts-node ../../scripts/build",
    "prepare": "yarn build",
    "prepack": "yarn prepare",
    "release": "dotenv -c production -- ts-node ../../scripts/release"
  },
  "dependencies": {
    "@flex-development/dtag": "1.0.0",
    "@flex-development/exceptions": "2.0.1",
    "@flex-development/tutils": "3.1.0",
    "@types/git-semver-tags": "4.1.1",
    "@types/lodash": "4.14.172",
    "@types/semver": "7.3.8",
    "@types/shelljs": "0.8.9",
    "@types/standard-version": "7.0.1",
    "anymatch": "3.1.2",
    "chalk": "4.1.2",
    "class-transformer": "0.4.0",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.13.1",
    "git-semver-tags": "4.1.1",
    "isomorphic-git": "1.9.2",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "reflect-metadata": "0.1.13",
    "shelljs": "0.8.4",
    "standard-version": "9.3.1",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@types/node": "16.4.13",
    "copyfiles": "2.4.1",
    "dotenv-cli": "4.0.0",
    "ts-node": "10.1.0",
    "ttypescript": "1.5.12"
  },
  "peerDependencies": {
    "@types/node": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=15.0.0",
    "yarn": "3.0.0-rc.2"
  },
  "env": {
    "optional": [],
    "required": []
  }
}
